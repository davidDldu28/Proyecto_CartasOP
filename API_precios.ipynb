    "import mysql.connector\n",
    "def send_to_mysql(data, database_name):\n",
    "    try:\n",
    "        connection = mysql.connector.connect(\n",
    "            host=\"localhost\",\n",
    "            user=\"root\",\n",
    "            password=\"miclave\",\n",
    "            database=database_name\n",
    "        )\n",
    "        cursor = connection.cursor()\n",
    "\n",
    "        # Crear tabla si no existe\n",
    "        cursor.execute(\"\"\"\n",
    "        CREATE TABLE IF NOT EXISTS One_Piece_Card_Prices (\n",
    "            id INT PRIMARY KEY,\n",
    "            Card_id INT,\n",
    "            Collector_number VARCHAR(100),\n",
    "            Card_Name VARCHAR(100),\n",
    "            Expansion_Code VARCHAR(100),\n",
    "            Rarity VARCHAR(100),\n",
    "            User_id INT,\n",
    "            User_name VARCHAR(100),\n",
    "            User_country VARCHAR(100),\n",
    "            Quantity VARCHAR(100),\n",
    "            Price DECIMAL(10,2)\n",
    "        )\n",
    "        \"\"\")\n",
    "\n",
    "        # Insertar datos\n",
    "        columns = ', '.join(data.columns)\n",
    "        values = ', '.join(['%s'] * len(data.columns))  \n",
    "        update_values = ', '.join([f\"{col}=VALUES({col})\" for col in data.columns if col != 'id'])\n",
    "        for _, row in data.iterrows():\n",
    "            cursor.execute(f\"\"\"\n",
    "            INSERT INTO One_Piece_Card_Prices ({columns}) \n",
    "            VALUES ({values}) \n",
    "            ON DUPLICATE KEY UPDATE {update_values}\n",
    "            \"\"\", (tuple(row)))\n",
    "\n",
    "        connection.commit()\n",
    "        print(\"Datos insertados en MySQL.\")\n",
    "    except mysql.connector.MySQLError as e:\n",
    "        print(\"Error en MySQL:\", e)\n",
    "    finally:\n",
    "        if connection:\n",
    "            cursor.close()\n",
    "            connection.close()\n"
   ]
